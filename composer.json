{
    "name": "squadms/default-theme",
    "description": "The default theme for SquadMS.",
    "keywords": ["laravel", "squad", "squadms", "default-theme"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://pkg.skyraptor.eu"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "squadms/foundation": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\DefaultTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\DefaultTheme\\SquadMSDefaultThemeServiceProvider"
            ]
        }
    }
}
